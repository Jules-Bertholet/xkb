default partial xkb_types "custom" {

    virtual_modifiers LevelThree, LevelFive;

    type "EIGHT_LEVEL_35_SEMIALPHABETIC" {
        modifiers = Shift + Lock + LevelThree + LevelFive;
        map[None] = Level1;
        map[Lock] = Level1;
        map[Shift] = Level2;
        map[Shift+Lock] = Level2;
        map[LevelThree] = Level3;
        map[Lock+LevelThree] = Level5;
        map[Shift+LevelThree] = Level4;
        map[Shift+Lock+LevelThree] = Level4;
        map[LevelFive] = Level5;
        map[Lock+LevelFive] = Level3;
        map[Shift+LevelFive] = Level6;
        map[Shift+Lock+LevelFive] = Level6;
        map[LevelThree+LevelFive] = Level7;
        map[Lock+LevelThree+LevelFive] = Level7;
        map[Shift+LevelThree+LevelFive] = Level8;
        map[Shift+Lock+LevelThree+LevelFive] = Level8;
        preserve[Lock] = Lock;
        preserve[Shift+Lock] = Lock;
        preserve[Lock+LevelThree] = None;
        preserve[Shift+Lock+LevelThree] = Lock;
        preserve[Lock+LevelFive] = None;
        preserve[Shift+Lock+LevelFive] = Lock;
        preserve[Lock+LevelThree+LevelFive] = Lock;
        preserve[Shift+Lock+LevelThree+LevelFive] = Lock;
        level_name[Level1] = "Base";
        level_name[Level2] = "Shift";
        level_name[Level3] = "AltGr";
        level_name[Level4] = "Shift AltGr";
        level_name[Level5] = "X";
        level_name[Level6] = "X Shift";
        level_name[Level7] = "X AltGr";
        level_name[Level8] = "X Shift AltGr";
    };

    type "EIGHT_LEVEL_56_SEMIALPHABETIC" {
        modifiers = Shift + Lock + LevelThree + LevelFive;
        map[None] = Level1;
        map[Lock] = Level1;
        map[Shift] = Level2;
        map[Shift+Lock] = Level2;
        map[LevelThree] = Level3;
        map[Lock+LevelThree] = Level3;
        map[Shift+LevelThree] = Level4;
        map[Shift+Lock+LevelThree] = Level4;
        map[LevelFive] = Level5;
        map[Lock+LevelFive] = Level6;
        map[Shift+LevelFive] = Level6;
        map[Shift+Lock+LevelFive] = Level5;
        map[LevelThree+LevelFive] = Level7;
        map[Lock+LevelThree+LevelFive] = Level7;
        map[Shift+LevelThree+LevelFive] = Level8;
        map[Shift+Lock+LevelThree+LevelFive] = Level8;
        preserve[Lock] = Lock;
        preserve[Shift+Lock] = Lock;
        preserve[Lock+LevelThree] = Lock;
        preserve[Shift+Lock+LevelThree] = Lock;
        preserve[Lock+LevelFive] = None;
        preserve[Shift+Lock+LevelFive] = None;
        preserve[Lock+LevelThree+LevelFive] = Lock;
        preserve[Shift+Lock+LevelThree+LevelFive] = Lock;
        level_name[Level1] = "Base";
        level_name[Level2] = "Shift";
        level_name[Level3] = "AltGr";
        level_name[Level4] = "Shift AltGr";
        level_name[Level5] = "X";
        level_name[Level6] = "X Shift";
        level_name[Level7] = "X AltGr";
        level_name[Level8] = "X Shift AltGr";
    };

    type "SEVEN_LEVEL" {
        modifiers = Shift + LevelThree + LevelFive;
        map[None] = Level1;
        map[Shift] = Level1;
        map[LevelThree] = Level2;
        map[Shift+LevelThree] = Level3;
        map[LevelFive] = Level4;
        map[Shift+LevelFive] = Level5;
        map[LevelThree+LevelFive] = Level6;
        map[Shift+LevelThree+LevelFive] = Level7;
        preserve[Shift] = Shift;
        preserve[Shift+LevelThree] = None;
        preserve[Shift+LevelFive] = None;
        preserve[Shift+LevelThree+LevelFive] = None;
        level_name[Level1] = "Base";
        level_name[Level2] = "AltGr";
        level_name[Level3] = "Shift AltGr";
        level_name[Level4] = "X";
        level_name[Level5] = "X Shift";
        level_name[Level6] = "X AltGr";
        level_name[Level8] = "X Shift AltGr";
    };

    type "KEYPAD_ALTGR" {
        modifiers = Shift + LevelThree + NumLock;

        map[None] = Level1;
        map[Shift] = Level1;
        preserve[Shift] = Shift;
        level_name[Level1] = "Base";

        map[LevelThree] = Level2;
        map[Shift+LevelThree] = Level2;
        preserve[LevelThree] = None;
        preserve[Shift+LevelThree] = Shift;
        level_name[Level2] = "AltGr";

        map[NumLock] = Level3;
        level_name[Level3] = "Number";

        map[Shift+NumLock] = Level4;
        preserve[Shift+NumLock] = None;
        level_name[Level4] = "Shift Number";

        map[LevelThree+NumLock] = Level5;
        level_name[Level5] = "AltGr Number";

        map[Shift+LevelThree+NumLock] = Level6;
        preserve[Shift+LevelThree+NumLock] = None;
        level_name[Level6] = "Shift AltGr Number";
    };
};
